<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <record id="auto_process_edi_transactions_cronjob" model="ir.cron">
            <field name="name">Auto Process EDI Transactions</field>
            <field name="model_id" ref="model_edi_transactions"/>
            <field name="state">code</field>
            <field name="code">model.auto_process_edi_transactions()</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>

        <record id="export_edi_transactions_cronjob" model="ir.cron">
            <field name="name">Export EDI Transactions</field>
            <field name="model_id" ref="model_edi_config_table"/>
            <field name="state">code</field>
            <field name="code">model.export_edi_transactions()</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>

        <record id="auto_delete_edi_ftp_log_message" model="ir.cron">
            <field name="name">EDI: Delete Log Message (Older than 13 days)</field>
            <field name="model_id" ref="model_log_book"/>
            <field name="state">code</field>
            <field name="code">model.auto_delete_log_message()</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
    </data>
</odoo>
